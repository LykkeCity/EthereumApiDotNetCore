<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.1</Version>
    <PackageId>Lykke.Service.EthereumCore.Client</PackageId>
    <Authors>Lykke</Authors>
    <Company>Lykke</Company>
    <Copyright>Lykke 2017</Copyright>
    <PackageLicenseUrl>https://github.com/LykkeCity/EthereumApiDotNetCore</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/LykkeCity/EthereumApiDotNetCore</PackageProjectUrl>
    <RepositoryUrl>https://github.com/LykkeCity/EthereumApiDotNetCore.git</RepositoryUrl>
    <PackageTags>Lykke Ethereum Eth EthereumCore EthereumApiDotNetCore Lykke.EthereumCoreClient EthereumCoreClient</PackageTags>
    <NeutralLanguage>en-001</NeutralLanguage>
    <Description>Http Client for EthereumApiDotNetCore</Description>
    <AssemblyName>Lykke.Service.EthereumCore.Client</AssemblyName>
    <RootNamespace>Lykke.Service.EthereumCore.Client</RootNamespace>
    <PackageIconUrl>https://avatars3.githubusercontent.com/u/14153330?v=3&amp;s=200</PackageIconUrl>
    <RepositoryType>Git</RepositoryType>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
  </ItemGroup>


  <ItemGroup>
    <PackageReference Include="Microsoft.Rest.ClientRuntime" Version="2.3.12" />
  </ItemGroup>

</Project>
